- content_for :head do
  :javascript
    $(document).ready(function () {
      var tagList = $("#destination_tag_list");
      tagList.autoSuggest('#{tags_path}/', {
        asHtmlID: "tags",
        neverSubmit: true,
        retriveLimit: 10,
        asHtmlID: "tags",
        minChars: 2,
        keyDelay: 200,
        startText: "",
        emptyText: "no_results",
        preFill: "#{@tag_list}"
        });

      var nameList = $("#destination_name_list");
      nameList.autoSuggest('#{tags_path}/', {
        asHtmlID: "tags",
        neverSubmit: true,
        retriveLimit: 10,
        asHtmlID: "names",
        minChars: 2,
        keyDelay: 200,
        startText: "",
        emptyText: "no_results",
        preFill: "#{@name_list}"
        });

    });
    

= form_for(@destination, :html => {:class => 'new_place_for'}, :validate => true) do |f|
  %fieldset
    .clearfix
    
      %b
        = t('destinations.title')
      = f.text_field :title
      %b
        = t('destinations.summary')
      = f.text_area :summary
      %b
        = t('destinations.tag_list')
      = f.text_field :tag_list, :rows => 5
      %b
        = t('destinations.name_list')
      = f.text_field :name_list, :rows => 5
      %b
        = t('destinations.photo_url')
      = f.text_field :photo_url

  .submit_field
    = f.submit t('destinations.submit'), :class => 'in_aspects'
